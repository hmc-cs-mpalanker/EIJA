<p id="notice"><%= notice %></p>

<h1>Cue Scripts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cue_scripts.each do |cue_script| %>
      <tr>
        <td><%= link_to 'Show', cue_script %></td>
        <td><%= link_to 'Edit', edit_cue_script_path(cue_script) %></td>
        <td><%= link_to 'Destroy', cue_script, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cue Script', new_cue_script_path %>
