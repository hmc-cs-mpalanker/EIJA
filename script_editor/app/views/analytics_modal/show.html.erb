a<div class="tab-content" id="myTabContent">

  <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
    <table class="table">
      <thead>
      <tr>
        <th scope="col">Character</th>
        <th scope="col"># Line numbers</th>
      </tr>
      </thead>
      <!--this data needs to be loaded from a different page or a json object sent from the server-->
      <tbody>
      <% @hash.each do |speaker| %>
        <% a = speaker %>
        <% spk = a[0] %>
        <% lineNum = a[1] %>
        <tr>
          <td> <%= spk %></td>
          <td> <%= lineNum.to_s() %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>

  <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
    <div class="form-group">
      <label for="exampleFormControlSelect1">Select Cue Script</label>
      <select class="form-control" id="queScriptNameSelector">
        <% @speakers.each do |speaker, value| %>
          <option value=<%= value %> > <%=speaker.titleize  %></option>
          <% end %>
      </select>
    </div>
    <button type="button" class="btn btn-primary" id="cueScriptSelect">Generate Cue Script</button>
  </div>


</div>


